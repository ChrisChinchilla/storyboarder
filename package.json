    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.23.9",
    "@tweenjs/tween.js": "^23.1.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "dotenv": "16.4.4",
